{
  "name": "swayer-starter-app",
  "version": "1.1.4",
  "description": "Initial Swayer application",
  "type": "module",
  "types": "types/index.d.ts",
  "scripts": {
    "start": "swr serve",
    "start:ssr": "swr serve --mode ssr",
    "start:dev": "swr serve -eP /node_modules/swayer/index.js",
    "linkEngine": "npm link ../../",
    "build": "swr build && swr spa dist",
    "build:prod": "swr build --prod && swr spa dist",
    "test": "npm run lint && tsc",
    "lint": "eslint .",
    "fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "eslint": "^8.34.0",
    "globals": "^13.20.0",
    "typescript": "^4.9.5"
  }
}
